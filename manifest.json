{
  "manifest_version": 2,

  "name": "No I Wasnt",
  "description": "This extension closes all porn tabs",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
   "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}